    "from BNReasoner import BNReasoner\n",
    "from BayesNet import BayesNet\n",
    "import networkx as nx\n",
    "\n",
    "net = BayesNet()\n",
    "bn = BNReasoner(net)\n",
    "\n",
    "net.load_from_bifxml('testing/lecture_example.BIFXML')\n",
    "net.get_all_variables()\n",
    "graph = net.get_interaction_graph()\n",
    "#net.draw_structure()"
   ]
